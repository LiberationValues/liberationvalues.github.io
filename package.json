{
	"devDependencies": {
		"@types/node": "^18.19.123",
		"@types/pug": "^2.0.10",
		"pug": "^3.0.3",
		"sass": "^1.91.0",
		"terser": "^5.43.1",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2"
	},
	"scripts": {
		"build": "tsc && sass src:dist && tsx ./build/build.ts",
		"release": "tsc && sass src:dist --style=compressed --no-source-map && tsx ./build/build.ts --minify --delete",
		"sass-watch": "sass -w src:dist",
		"tsc-watch": "tsc --watch"
	},
	"type": "module",
